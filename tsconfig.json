{
  "compilerOptions": {
    "target": "ES2022",
    "alwaysStrict": true,
    "esModuleInterop": true,
    "lib": [
      "ES2022",
      "DOM"
    ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "isolatedModules": true,
    "experimentalDecorators": true,
    "types": [
      "vite/client"
    ]
  }
}
